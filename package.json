{
  "name": "kinesin",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "watch": "npm-watch roll",
    "serve": "vue-cli-service serve",
    "dev": "run-p watch serve",
    "roll": "rollup -c",
    "build": "vue-cli-service build --target lib --name kinesin src/components/Kinesin.vue && npm run roll",
    "build-demo": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "publish:patch": "npm run build && npm version patch && npm publish",
    "publish:minor": "npm run build && npm version minor && npm publish",
    "publish:major": "npm run build && npm version major && npm publish"
  },
  "watch": {
    "roll": "src/components/Kinesin.vue"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "animejs": "^3.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "core-js": "^2.6.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "rollup": "^1.27.5",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^5.1.1",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.5.21"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "main": "dist/kinesin.common.js",
  "module": "dist/kinesin.esm.js",
  "unpkg": "dist/kinesin.umd.min.js",
  "browser": {
    "./sfc": "./src/components/Kinesin.vue"
  },
  "files": [
    "dist/*",
    "src/components/Kinesin.vue"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "license": "MIT"
}
